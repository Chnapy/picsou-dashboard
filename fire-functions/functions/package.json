{
  "name": "functions",
  "scripts": {
    "build": "rimraf lib/ && tsc --project tsconfig.prod.json",
    "serve": "yarn build && firebase emulators:start --only functions",
    "shell": "yarn build && firebase functions:shell",
    "start": "yarn shell",
    "test": "jest --watchAll",
    "test-ci": "jest",
    "test-integration": "yarn build  && firebase emulators:exec \"cd ../.. && yarn test\"",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "10.14.2"
  },
  "main": "lib/fire-functions/functions/src/index.js",
  "dependencies": {
    "date-fns": "2.16.1",
    "firebase-admin": "8.10.0",
    "firebase-functions": "3.6.1",
    "jsdom": "16.4.0",
    "node-fetch": "2.6.1"
  },
  "devDependencies": {
    "@types/jest": "26.0.13",
    "@types/jsdom": "16.2.4",
    "firebase-functions-test": "0.2.0",
    "jest": "26.4.2",
    "rimraf": "3.0.2",
    "ts-jest": "26.3.0",
    "typescript": "4.0.2"
  },
  "private": true
}
