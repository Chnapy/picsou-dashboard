{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "lib": [
            "ES2020"
        ],
        "strict": true,
        "esModuleInterop": true,
        "downlevelIteration": true,
        "skipLibCheck": true
    }
}
